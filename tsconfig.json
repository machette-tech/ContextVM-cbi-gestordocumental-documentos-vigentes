{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2021"],
    "moduleResolution": "node16",
    "ignoreDeprecations": "6.0",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./cbi-gestordocumental-documentos-vigentes/dist",
    "rootDir": "./cbi-gestordocumental-documentos-vigentes/src",
    "types": ["node"]
  },
  "include": ["cbi-gestordocumental-documentos-vigentes/src/**/*"],
  "exclude": ["node_modules", "dist", "tests", "fsm-designer"]
}
